{
  "name": "whatwebcando",
  "description": "Can I rely on the Web Platform features to build my app? An overview of the device integration HTML5 APIs",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "mv dist/.git __sapper__/_tmp_dist_git && (sapper export dist --legacy || true) && mv __sapper__/_tmp_dist_git dist/.git",
    "export-aliases": "node deploy/aliases.mjs",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "deploy": "(cd dist && git add --ignore-removal . && git commit -m \"Update to $(cd .. && git rev-parse HEAD)\" && git push && git reseth)",
    "deploy-staging": "surge ./dist https://wwcd-beta.surge.sh"
  },
  "dependencies": {
    "chartist": "^0.11.4",
    "chartist-plugin-tooltips": "0.0.17",
    "compare-versions": "^2.0.1",
    "compression": "^1.7.1",
    "marked": "^0.8.0",
    "parse-md": "^2.0.3",
    "polka": "next",
    "prismjs": "^1.21.0",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.1",
    "sapper": "git://github.com/NOtherDev/sapper.git#b05aceea8b2d5aaf91e7fcc7df646dbafd76907b",
    "surge": "^0.21.6",
    "svelte": "^3.0.0",
    "svelte-preprocess-sass": "^0.2.0"
  },
  "resolutions": {
    "minimist": "1.2.5"
  }
}
